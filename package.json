{
  "name": "mat-rel",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "build": "npm run build:ssr && node sitemapGenerator",
    "start:proxy": "ng serve --proxy-config proxy.config.js",
    "start:dev": "npm start --sourceMap=false",
    "optmizecss": "gulp --gulpfile build-scripts/optimize-css.gulpfile.js",
    "build-website": "ng build --configuration production --aot --output-hashing=all --build-optimizer --optimization --base-href /",
    "build-website-2": "ng build --configuration production --aot --outputHashing=all  --build-optimizer --named-chunks --extract-css=true",
    "test": "ng test",
    "test:common": "ng test --browsers Chrome,Firefox",
    "test:ci": "ng test --watch=true --reporters junit --browsers ChromeHeadless",
    "test:cov": "ng test --watch=true --sourceMap=true --codeCoverage=true --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write .",
    "dev:ssr": "ng run mat-rel:serve-ssr",
    "serve:ssr": "node dist/mat-rel/server/main.js",
    "build:ssr": "npm run build-website && ng run mat-rel:server:production",
    "prerender": "ng run mat-rel:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/platform-server": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@nguniversal/express-engine": "^12.1.0",
    "bootstrap": "^5.1.1",
    "deep-equal": "^2.0.5",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "jwt-decode": "^3.1.2",
    "localstorage-polyfill": "^1.0.1",
    "rxjs": "~6.6.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@nguniversal/builders": "^12.1.0",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.3.2",
    "typescript": "~4.3.2"
  }
}
